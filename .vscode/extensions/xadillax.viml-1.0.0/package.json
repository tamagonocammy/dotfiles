{
	"name": "viml",
	"displayName": "VimL (Vim Language, Vim Script)",
	"version": "1.0.0",
	"description": "Vim Script language support for VSCode.",
	"scripts": {
		"update-grammar": "update-grammar Alhadis/language-viml grammars/viml.cson syntaxes/viml.tmLanguage.json"
	},
	"engines": {
		"vscode": "^1.20.0"
	},
	"categories": [
		"Programming Languages"
	],
	"publisher": "XadillaX",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/XadillaX/vscode-language-viml.git"
	},
	"author": "XadillaX <i@2333.moe>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/XadillaX/vscode-language-viml/issues"
	},
	"homepage": "https://github.com/XadillaX/vscode-language-viml#readme",
	"galleryBanner": {
		"color": "#569cd6",
		"theme": "dark"
	},
	"icon": "assets/logo.png",
	"devDependencies": {
		"vscode-update-grammar-tool": "^1.0.0"
	},
	"contributes": {
		"grammars": [
			{
				"language": "viml",
				"scopeName": "source.viml",
				"path": "./syntaxes/viml.tmLanguage.json"
			}
		],
		"languages": [
			{
				"id": "viml",
				"aliases": [
					"VimL",
					"VimScript",
					"Vim Language"
				],
				"extensions": [
					".vim",
					".vimrc",
					".gvim",
					".ideavim",
					".exrc"
				],
				"filenames": [
					"vimrc"
				],
				"configuration": "./language-configuration.json"
			}
		]
	},
	"__metadata": {
		"id": "ecc7bbf7-9d61-46c8-b47e-a2a27c38b38d",
		"publisherId": "9ea94209-a714-4c79-872e-95fcd2b2d718",
		"publisherDisplayName": "XadillaX"
	}
}